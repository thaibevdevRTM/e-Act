<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title e-ActForm RTM Sale Support</title>

    <link rel="icon" sizes="192x192" href="~/images/favicon.ico">
    <link rel="apple-touch-icon" sizes="128x128" href="~/images/logo-ss.png">
    <link rel="apple-touch-icon-precomposed" sizes="128x128" href="~/images/logo-ss.png">

    <style>
        .loading {
            position: fixed;
            z-index: 999;
            height: 2em;
            width: 2em;
            overflow: show;
            margin: auto;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
            background-color: rgba(0,0,0,0.5); /* Black w/ opacity */
            width: 100%;
            height: 100%;
        }
    </style>

    @Styles.Render("~/Content/themes/base/css")

    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/jquery")


</head>
<body>
    <div id="WaitDialog" class="loading" style="text-align: center;display:none;">
        <img src="~/images/loading.gif" style="margin-top:100px;width:100px;height:100px;" />
        <div style="margin-top: 10px; color: white">
            <b>Please wait</b>
        </div>
    </div>

    @RenderBody()

    @Scripts.Render("~/bundles/jqueryui", "~/bundles/bootstrap")

    <script src="~/Content/vendors/build/js/custom.js" type="text/javascript"></script>

    </script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script src="https://www.googletagmanager.com/gtag/js?id=UA-138769477-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-138769477-1');
    </script>

</body>




</html>
