@model eActForm.Models.ApproveModel.approveModels
@if (Model.approveDetailLists != null && Model.approveDetailLists.Count() > 0)
{


    <table class="tablethin" border="1" cellpadding="0" cellspacing="0" width="85%">
        <tr><td></td><td></td></tr>
        @foreach (var item in Model.approveDetailLists)
        {
            if (@item.remark != "")
            {
                <tr>
                    <td>
                        <div style="height:70px; font-size:12.5px; margin-top:10px; margin-left:20px;">
                            หมายเหตุ : @item.remark
                        </div>
                    </td>
                </tr>
            }
        }

        <tr>
            @foreach (var item in Model.approveFlowDetail)
            {
                <td>
                    <div style="text-align:center; margin-top:5px; font-size:12.5px;">@item.empFNameTH @item.empLNameTH</div>
                    <div style="text-align:center; height:30px; font-size:12.5px;">@item.empPositionTitleTH</div>
                </td>
            }
        </tr>

    </table>
}