@model eActForm.Models.RepDetailModel.actApproveRepDetailModels
<div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
        <div class="modal-header">
            <div style=""></div>
            <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
            <div id="Grid1" style="width:97%; margin-left:20px;">
                <div>
                    <table id="dtApproveLists" class="table table-striped table-hover">
                        <thead>
                            <tr style="background-color:#2B5D95;color:#ffffff;">
                                <td class="textRight"><h5>ActNo</h5></td>
                            </tr>
                            @foreach (var item in Model.repDetailLists)
                            {
                                <tr>
                                    <td>@Html.DisplayFor(x => item.activityNo)</td>
                                </tr>
                            }
                        </thead>
                    </table>
                </div>

            </div>
        </div>
        <div class="modal-footer">
            <div>
                <p class="d-flex justify-content-center">
                    <button type="button" class="btn btn-primary" data-dismiss="modal"><i class="fa fa fa-reply fa-fw"></i>&nbsp;Close</button>
                </p>
            </div>

        </div>
    </div>
</div>

