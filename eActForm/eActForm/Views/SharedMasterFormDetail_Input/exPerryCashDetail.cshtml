@using eActForm.BusinessLayer;
@model eActForm.Models.Activity_TBMMKT_Model



<div class="form-group col-md-12" id="divExpenseCash" style="margin-top:30px">
    <div class="col-lg-12 col-xs-12">
        <div class="col-md-12 col-xs-12">
            <div class="col-md-1 col-xs-12">
                <label class="control-label">
                    ประจำเดือน
                </label>
            </div>
            <div class="col-md-2 col-xs-12">
                @Html.TextBoxFor(m => m.activityFormModel.dateDoc, new { @class = "form-control textboxcss", @id = "txtExMonth", Style = "text-align:center;" })
            </div>

            <div class="col-md-2 col-xs-12">
                <label class="control-label">
                    เลขที่เอกสารใบยืมเงินทดลอง
                </label>
            </div>
            <div class="col-md-2 col-xs-12">
                @Html.TextBoxFor(m => m.activityFormModel.reference, new { @class = "form-control textboxcss", Style = "width:5;", @id = "txtReference" })

            </div>
        </div>
        <div class="col-md-12 col-xs-12">
           
        </div>
    </div>
</div>


<script type="text/javascript">


    $(function () {
        $("#txtExMonth").datepicker({
            format: "mm-yyyy",
            viewMode: "months",
            minViewMode: "months"
        });
    });



</script>
